!URxvt.font: *-*-*-*-*-16-*
URxvt.font:     xft:bitstream vera sans mono:size=9:antialias=false
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=9:antialias=false

URxvt.foreground:		 #0F928C
URxvt.background:		 #2C2E2F

(black)
URxvt.color0:	#494848
(bright black)
URxvt.color8: 	#706E6E

(red)
URxvt.color1:	#EE4F7C
(bright red)
URxvt.color9: 	#D3849A

(green)
URxvt.color2: 	#6CAD66
(bright green)
URxvt.color10: 	#66AD72

(yellow)
URxvt.color3: 	#BEEE3B
(bright yellow)
URxvt.color11:  #A0EE3B

(blue)
URxvt.color4:	#006465
(bright blue)
URxvt.color12: 	#005C65

(magenta)
URxvt.color5:   #493FD2
(bright magenta)
URxvt.color13:  #3F5AD2

(cyan)
URxvt.color6:	#0F928C
(bright cyan)
URxvt.color14: 	#64A5A2

(white)
URxvt.color7: 	#C0C0C0
(bright white)
URxvt.color15: 	#E9E9E9

URxvt*scrollBar: False
URxvt*saveLines: 100000
URxvt.urlLauncher     : firefox
URxvt*matcher.button:        1
URxvt.perl-lib: /usr/lib/URxvt/perl/
URxvt.perl-ext:  default,selection-to-clipboard,matcher
URxvt.iso14755 : false
