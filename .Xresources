!URxvt.font: *-*-*-*-*-16-*
URxvt.font:     xft:bitstream vera sans mono:size=9:antialias=false
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=9:antialias=false

URxvt*scrollBar: False
URxvt*saveLines: 100000
URxvt.url-launcher: firefox
URxvt*matcher.button: 1
URxvt.perl-lib: /usr/lib64/urxvt/perl
URxvt.perl-ext:  default,selection-to-clipboard,matcher
URxvt.iso14755 : false
URxvt.print-pipe: "cat > /dev/null" 

!http://dotshare.it/dots/169/
URxvt*background:        #333333
URxvt*foreground:        #cccccc
URxvt*cursorColor:       #999999
URxvt*colorBD:           #ffffff
URxvt*colorUL:           #ffff99

